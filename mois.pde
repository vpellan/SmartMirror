void mois() //Convertir la valeur numérique au mois correspondant (1=janvier, 2=février,...)
{
  if (month()==1)
  {
    mois="janvier";
  }
  else if (month()==2)
  {
    mois="février";
  }
  else if (month()==3)
  {
    mois="mars";
  }
  else if (month()==4)
  {
    mois="avril";
  }
  else if (month()==5)
  {
    mois="mai";
  }
  else if (month()==6)
  {
    mois="juin";
  }
  else if (month()==7)
  {
    mois="juillet";
  }
  else if (month()==8)
  {
    mois="août";
  }
  else if (month()==9)
  {
    mois="septembre";
  }
  else if (month()==10)
  {
    mois="octobre";
  }
  else if (month()==11)
  {
    mois="novembre";
  }
  else if (month()==12)
  {
    mois="décembre";
  }
}
