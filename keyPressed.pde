void keyPressed()
{ 
  if (keyCode == UP)
  {
    touche = "U";
  }
  else if (keyCode == DOWN)
  {
    touche = "D";
  }
  else if (keyCode == LEFT)
  {
    touche = "L";
  }
  else if (keyCode == RIGHT)
  {
    touche = "R";
  }
  else if (key == 'b' || key == 'B')
  {
    touche = "B";
  }
  else if (key == 'a' || key == 'A')
  {
    touche = "A";
  }
  else if (keyCode == ENTER)
  {
    touche = "Lock";
  }
}
